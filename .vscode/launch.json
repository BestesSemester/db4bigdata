    "version": "0.2.0",
    "configurations": [

        {
            "name": "neo4j importer run in Debug-Mode",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/importer_neo4j/main.go",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "MSSQL importer run in Debug-Mode",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/importer_mssql/main.go",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "MSSQL find run in Debug-Mode",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/find_mssql/main.go", 
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "mongo importer run in Debug-Mode",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/importer_mongo/main.go",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "mongo find run in Debug-Mode",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "args": ["1079", "2000-12-11", "2022-12-15"],
            "program": "${workspaceFolder}/cmd/find_mongo/main.go",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "mongo find optimized run in Debug-Mode",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "args": ["1079", "2000-12-11", "2022-12-15"],
            "program": "${workspaceFolder}/cmd/find_mongo_optimized/main.go",
            "cwd": "${workspaceFolder}"
        }
    ]
}